{% extends 'base.html.twig' %}

{% block title %}Emploi{% endblock %}

{% block body %}
    <h1>Emploi</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emploi.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ emploi.titre }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ emploi.adresse }}</td>
            </tr>
            <tr>
                <th>PublishedAt</th>
                <td>{{ emploi.publishedAt ? emploi.publishedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('emploi_index') }}">back to list</a>

    <a href="{{ path('emploi_edit', {'id': emploi.id}) }}">edit</a>

    {{ include('emploi/_delete_form.html.twig') }}
{% endblock %}
